{
    "name": "mahmud/parts-authority",
    "description": "PHP SDK for Parts Authority API",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "MD. Mahmud Ur Rahman",
            "email": "mahmudkuet11@gmail.com",
            "homepage": "https://mahmud.live"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^7.1.3",
        "guzzlehttp/guzzle": "~6.0",
        "illuminate/support": "^6.7",
      "ext-json": "*"
    },
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Mahmud\\PartsAuthority\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mahmud\\PartsAuthority\\Tests\\": "tests"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "keywords": ["Parts Authority"],
    "require-dev": {
        "phpunit/phpunit": "^7"
    }
}
